{
  "name": "huncwot-example-app",
  "description": "A Huncwot.js project",
  "version": "1.0.0",
  "author": "Zaiste <oh@zaiste.net>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "client": "vue-cli-service serve client/index.ts",
    "client:build": "vue-cli-service build client/index.ts",
    "client:deploy": "rsync -azP --rsh=ssh dist/ YOUR_LOCATION",
    "server": "ROARR_LOG=true huncwot server | npx roarr pretty-print",
    "server:dev": "nodemon --quiet -w server --exec 'tsc && npm run server'",
    "server:build": "tsc",
    "server:deploy": "rsync -azP --rsh=ssh --exclude 'dist' --exclude 'node_modules' --exclude '.git' --exclude '.build' --exclude 'config/database.json' . YOUR_LOCATION",
    "typescript:watch": "tsc -w",
    "start": "stmux -w always -e ERROR -m beep,system -- [ [ -t 'Client: Vue.js' 'npm run client' .. -t 'Server' 'npm run server:dev' ] : -t 'TypeScript' -s 1/3 -f \"npm run typescript:watch\" ]",
    "migrate": "node-pg-migrate -m db/migrations"
  },
  "dependencies": {
    "@vue/cli-service": "^3.6.0",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.14",
    "axios": "^0.18.0",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "huncwot": "0.38.0",
    "tspath": "^1.3.7",
    "validate": "^4.5.1",
    "vue": "2.6.10",
    "vue-apollo": "^3.0.0-beta.28",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@roarr/cli": "^1.3.1",
    "@types/node": "^11.13.5",
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-pwa": "^3.6.0",
    "@vue/cli-plugin-typescript": "^3.6.0",
    "@vue/cli-plugin-unit-mocha": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "node-pg-migrate": "^3.18.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "sass-loader": "^7.1.0",
    "stmux": "^1.6.1",
    "typescript": "^3.4.4",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
