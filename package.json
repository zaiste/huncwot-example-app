{
  "name": "huncwot-example-app",
  "description": "A Huncwot.js project",
  "version": "1.0.0",
  "author": "Zaiste <oh@zaiste.net>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "client": "vue-cli-service serve client/index.ts",
    "build": "vue-cli-service build client/index.ts",
    "server": "huncwot server",
    "server:dev": "nodemon --quiet -w server --exec 'tsc && huncwot server'",
    "watch-ts": "tsc -w",
    "compile": "tsc",
    "start": "npm-run-all --silent compile -p client server:dev watch-ts",
    "deploy:client": "rsync -azP --rsh=ssh dist/ YOUR_LOCATION",
    "deploy:server": "rsync -azP --rsh=ssh --exclude 'dist' --exclude 'node_modules' --exclude '.git' --exclude '.build' --exclude 'config/database.json' . YOUR_LOCATION"
  },
  "dependencies": {
    "@vue/cli-service": "^3.3.0",
    "apollo-cache-inmemory": "1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link-http": "^1.5.9",
    "axios": "^0.18.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "huncwot": "0.28.1",
    "tspath": "^1.3.7",
    "validate": "^4.5.1",
    "vue": "2.5.21",
    "vue-apollo": "^3.0.0-beta.26",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@types/node": "^10.12.18",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-pwa": "^3.3.0",
    "@vue/cli-plugin-typescript": "^3.3.0",
    "@vue/cli-plugin-unit-mocha": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "typescript": "^3.2.2",
    "vue-loader": "^15.5.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "2.5.21",
    "webpack": "4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
